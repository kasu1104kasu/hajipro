<h2 class="all-lists"> New List </h2>
<div class="form">
<!-- form_with部分 -->
<%= form_with model: @list, url:'/todolists', local:true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <%= f.submit '投稿' %>
<% end %>
</div>